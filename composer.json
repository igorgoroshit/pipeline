{
    "name": "igorgoroshit/pipeline",
    "description": "A very simple and easy to use asset pipeline for laravel 4.",
    "type": "library",
    "version": "5.2.1",
    "license": "MIT",

    "authors": [
        {
            "name": "Kelt Dockins",
            "email": "kelt@dockins.org"
        },
        {
            "name": "Igor Goroshit",
            "email": "igor@webt.co.il"
        }
    ],
    
    "repositories": [
		{
			"type": "git",
			"url": "https://github.com/igorgoroshit/sprockets"
		}
    ],
    
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0|~5.0",
        "igorgoroshit/sprockets": "5.2.1",
        "kriswallsmith/assetic": "v1.1.2",
        "ext-fileinfo": "*"
    },

    "require-dev": {
        "laravel/framework": "4.*"
    },
    
    "autoload": {

        "classmap": [
            "src/controllers"
        ],
        
        "psr-0": {
            "Igorgoroshit\\Pipeline": "src/"
        }
    }
}
